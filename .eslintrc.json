{
    "rules": {
        "import/no-anonymous-default-export": "off",
        "react/jsx-curly-brace-presence": ["error", "never"],
        "react/self-closing-comp": "error",
        "no-new-func": "off",
        "default-case": "off"
    },
    "extends": [
        "react-app",
        "plugin:putout/safe+align",
        "react-app/jest"
    ],
    "plugins": ["putout"]
}
