{
    "printer": "putout",
    "rules": {
        "remove-console": "off",
        "convert-commonjs-to-esm": "off",
        "convert-esm-to-commonjs": "off",
        "package-json/add-type": "off",
        "promises/add-missing-await": "off",
        "nodejs": "off"
    },
    "plugins": [
        "react-hooks"
    ]
}
